  "query": "\n            SELECT\n            r.id,\n            r.name,\n            r.description,\n            r.steps,\n            r.time,\n            r.servings\n            FROM recipes AS r\n            JOIN ingredients_recipes AS ir ON r.id = ir.recipe_id\n            JOIN ingredients AS i ON ir.ingredient_id = i.id\n            WHERE r.id = $1\n            ",
